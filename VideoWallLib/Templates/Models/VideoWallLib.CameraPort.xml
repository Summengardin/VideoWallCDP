<?xml version="1.0" encoding="utf-8"?>

<Model Name="VideoWallLib.CameraPort">

  <ModelTypeClass>C++</ModelTypeClass>
  <BaseModel>CDPPort</BaseModel>

  <Attributes>
    <Attribute Name="Description" Value="Simple port example" Type="string"></Attribute>
    <Attribute Name="Model" Value="VideoWallLib.CameraPort" Type="string" Description="Implementation model used." SaveOnChange="0" Required="1" ReadOnly="1"></Attribute>
    <Attribute Name="Width" RouteMethod="RoutePeriodic" Type="int" Value="1920"></Attribute>
    <Attribute Name="Height" Value="1080" Type="int"></Attribute>
    <Attribute Name="IP" Value="10.1.3.70"></Attribute>
    <Attribute Name="Framerate" Type="float" Value="54"></Attribute>
    <Attribute Name="DisplayName"></Attribute>
    <Attribute Name="Format"></Attribute>
  </Attributes>

  <Connections>
    <Connection Model="CDPPortConnection" Name="MapWidth" Input="1" LocalName=".Width"></Connection>
    <Connection Model="CDPPortConnection" Name="MapHeight" Input="1" LocalName=".Height"></Connection>
    <Connection Model="CDPPortConnection" Name="MapIP" Input="1" LocalName=".IP"></Connection>
    <Connection Model="CDPPortConnection" Name="MapFramerate" Input="1" LocalName=".Framerate"></Connection>
    <Connection Model="CDPPortConnection" Name="MapDisplayName" Input="1" LocalName=".DisplayName"></Connection>
    <Connection Model="CDPPortConnection" Name="MapFormat" Input="1" LocalName=".Format"></Connection>
  </Connections>
</Model>