    <Signal Input="1" Model="CDPSignal&lt;string&gt;" Name="Mode" Type="string" Routing="DataSimulation.GWMode.Out"></Signal>
    <Signal Description="Shows description of current OSD elements instead of data" Input="1" Model="CDPSignal&lt;bool&gt;" Name="ShowOSDDescription" Type="bool" Unit="0 / 1"></Signal>
  </Signals>

  <Alarms>
