    <Signal Description="Process interval [s]." Input="0" Model="CDPSignal&lt;double&gt;" Name="Process Period" Type="double" Unit="s"></Signal>
    <Signal Description="Current load measured on Gangway tip" Input="1" Model="CDPSignal&lt;double&gt;" Name="GangwayLoad" Type="double" Unit="t" Routing="DataSimulation.GWLoad.Out"></Signal>
